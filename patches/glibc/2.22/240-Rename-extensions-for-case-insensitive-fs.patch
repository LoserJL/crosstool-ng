+CFLAGS-.oSTATIC = $(CFLAGS-.o) $(PIC-ccflag)
+CPPFLAGS-.oSTATIC = $(CPPFLAGS-.o) -DPIC -DLIBC_NONSHARED=1
+libtype.oSTATIC = lib%_nonshared.a
 endif
 
