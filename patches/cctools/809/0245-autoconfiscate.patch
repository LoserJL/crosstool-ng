+	$(CC) -o $@ $^ $(MYLINKFLAGS)
+seg_addr_table: $(OBJS_seg_addr_table)
+	$(CC) -o $@ $^ $(MYLINKFLAGS)
+seg_hack: $(OBJS_seg_hack)
+	$(CC) -o $@ $^ $(MYLINKFLAGS)
+install_name_tool: $(OBJS_install_name_tool)
+	$(CC) -o $@ $^ $(MYLINKFLAGS)
+indr: $(OBJS_indr)
+	$(CC) -o $@ $^ $(MYLINKFLAGS)
+strip: $(OBJS_strip)
+	$(CXX) -o $@ $^ $(MYLINKFLAGS) -L$(top_builddir)/libprunetrie -lprunetrie
