CT_TARGET_VENDOR="apple"
#CT_ARCH_ARCH="armv6"
CT_ARCH_FLOAT_SW=y
CT_ARCH_arm=y
CT_MULTILIB=n
CT_LLVM_V_2336=y
#
# Build system
#
CT_BUILD=""
CT_BUILD_PREFIX=""
CT_BUILD_SUFFIX=""
CT_KERNEL_darwin=y
CT_DARWIN_IOS_V_6_1=y
