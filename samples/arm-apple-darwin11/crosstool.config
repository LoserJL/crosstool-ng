CT_EXPERIMENTAL=y
CT_STATIC_TOOLCHAIN=y
CT_DEBUG_CT=y
CT_DEBUG_CT_SAVE_STEPS=y
CT_DEBUG_CT_SAVE_STEPS_GZIP=y
CT_LOCAL_TARBALLS_DIR="${HOME}/src"
CT_SAVE_TARBALLS=y
CT_LOG_EXTRA=y
CT_LOG_TO_FILE=y
CT_EXTRA_CFLAGS_FOR_HOST="-m32"
